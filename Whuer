local function white()
---====== Load spawner ======---

local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/Voor-Pr00/Fogroom/refs/heads/main/Spawner%20v3"))()
function sexyspawn()
    local v1 = game:GetService("TweenService")
    local v2 = TweenInfo.new(2)
    local v3 = Instance.new("ColorCorrectionEffect")
    v3.Parent = game.Lighting
    v3.Brightness = 0.05
    v3.Contrast = 0
    v3.Name = "Jen"
    v3.Enabled = true
    v3.TintColor = Color3.fromRGB(255, 255, 255)
    v3.Saturation = 0
    v1:Create(v3, v2, {
        Contrast = 1,
        Saturation = 1,
        TintColor = Color3.fromRGB(255, 0, 0)
    }):Play()
    wait(2)
    v1:Create(v3, v2, {
        Contrast = 0,
        Saturation = 0,
        TintColor = Color3.fromRGB(255, 255, 255),
        Brightness = 0
    }):Play()
    wait(4)
    v3:Destroy()
end
---====== Create entity ======---
local entity = spawner.Create({
	Entity = {
		Name = "WH1T3", 
		Asset = "https://github.com/PABMAXICHAC/doors-monsters-models/blob/main/white_part2.rbxm?raw=true",
		HeightOffset = 0
	},
	Lights = {
		Flicker = {
			Enabled = false,
			Duration = 1
		},
		Shatter = true,
		Repair = true
	},
	Earthquake = {
		Enabled = false
	},
	CameraShake = {
		Enabled = true,
		Range = 100,
		Values = {10.5, 30, 1.1, 5} -- Magnitude, Roughness, FadeIn, FadeOut
	},
	Movement = {
		Speed = 350,
		Delay = 6.5,
		Reversed = false
	},
	Rebounding = {
		Enabled = true,
		Type = "Ambush",
		Min = 2,
		Max = 2,
		Delay = 0
	},
	Damage = {
		Enabled = true,
		Range = 74,
		Amount = 100
	},
	Crucifixion = {
		Enabled = true,
		Range = 68,
		Resist = false,
		Break = true
	},
	Death = {
		Type = "Guiding", -- "Curious"
		Hints = {"Death", "Hints", "Go", "Here"},
		Cause = ""
	}
})

---====== Debug entity ======---

entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
local v142 = Instance.new("Sound")
    v142.SoundId = "rbxassetid://" .. 131489490
    v142.Looped = false
    v142.Parent = workspace
    v142.Volume = 3.1
    v142:Play()
local room = workspace.CurrentRooms[game.Players.LocalPlayer:GetAttribute("CurrentRoom")]
local color = Color3.fromRGB(255, 0, 0)
		room.LightBase.SurfaceLight.Enabled = true
		room.LightBase.SurfaceLight.Color = color
		for _, thing in pairs(room.Assets:GetDescendants()) do
			if thing:FindFirstChild"LightFixture" then
				thing.LightFixture.Neon.Color = color
				for _, light in pairs(thing.LightFixture:GetChildren()) do
					if light:IsA("SpotLight") or light:IsA("PointLight") then
						light.Color = color
					end
				end
			end
		end
    sexyspawn()
end)

entity:SetCallback("OnStartMoving", function()
    print("Entity has started moving")
end)

entity:SetCallback("OnEnterRoom", function(room, firstTime)
    if firstTime == true then
        print("Entity has entered room: ".. room.Name.. " for the first time")
    else
        print("Entity has entered room: ".. room.Name.. " again")
    end
end)

entity:SetCallback("OnLookAt", function(lineOfSight)
		print("Player view is obstructed by something")
local v144 = tick()
    local v145 = 5
    while v144 + 1 / v145 > tick() do
    end
    wait()
    tick()
end)

entity:SetCallback("OnRebounding", function(startOfRebound)
    -- Setting
	local entityModel = entity.Model
	local main = entityModel:WaitForChild("Main")
	local attachment = main:WaitForChild("Attachment")
	local AttachmentSwitch = main:WaitForChild("AttachmentSwitch")
	local sounds = {
		footsteps = main:WaitForChild("Footsteps"),
		playSound = main:WaitForChild("PlaySound"),
		switch = main:WaitForChild("Switch"),
		switchBack = main:WaitForChild("SwitchBack")
	}
	
	-- Faster Rebound
	for _, c in attachment:GetChildren() do
		c.Enabled = (not startOfRebound)
	end
	for _, c in AttachmentSwitch:GetChildren() do
		c.Enabled = startOfRebound
	end
end)

entity:SetCallback("OnDespawning", function()
    print("Entity is despawning")
local room = workspace.CurrentRooms[game.Players.LocalPlayer:GetAttribute("CurrentRoom")]
local color = Color3.fromRGB(235, 167, 98)
		room.LightBase.SurfaceLight.Enabled = true
		room.LightBase.SurfaceLight.Color = Color3.fromRGB(89, 69, 72)
		for _, thing in pairs(room.Assets:GetDescendants()) do
			if thing:FindFirstChild"LightFixture" then
				thing.LightFixture.Neon.Color = Color3.fromRGB(195, 161, 141)
				for _, light in pairs(thing.LightFixture:GetChildren()) do
					if light:IsA("SpotLight") or light:IsA("PointLight") then
						light.Color = color
					end
				end
			end
		end
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
	else
		print("Entity has damaged the player")
wait(13)
    game:Shutdown()
end)

---====== Run entity ======---

entity:Run()
end

if game.Players.LocalPlayer.Character.Humanoid.Health >= 0 then
white()
end

